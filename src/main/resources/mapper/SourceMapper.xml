<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cc.jiusi.blog.mapper.SourceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cc.jiusi.blog.entity.po.Source">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="remark" property="remark" />
        <result column="type" property="type" />
        <result column="md5" property="md5" />
        <result column="uid" property="uid" />
        <result column="create_by" property="createBy" />
        <result column="create_time" property="createTime" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
        <result column="del_flag" property="delFlag" />
        <result column="real_name" property="realName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, remark, type, md5, uid, create_by, create_time, update_by, update_time, del_flag, real_name
    </sql>

</mapper>
